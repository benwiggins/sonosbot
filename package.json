{
  "name": "sonosbot",
  "version": "0.1.0",
  "description": "Control Sonos/Spotify through Slack.",
  "main": "index.js",
  "repository": "https://github.com/benwiggins/sonosbot.git",
  "author": "Ben Wiggins",
  "license": "MIT",
  "scripts": {
    "start": "DEBUG=sonosbot:* node index.js",
    "lint": "eslint src",
    "prettier": "prettier --write '**/*.js'",
    "test": "NODE_ENV=${NODE_ENV:=test} DEBUG=sonosbot:*,test:* ./node_modules/.bin/mocha --recursive --exit --timeout 10000",
    "test-coverage": "nyc yarn test",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "lint-staged": {
    "**/*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "chai": "4.2.0",
    "chai-spies": "^1.0.0",
    "dirty-chai": "2.0.1",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-prettier": "3.0.1",
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "mocha": "6.1.2",
    "nock": "10.0.6",
    "nyc": "13.3.0",
    "prettier": "1.16.4"
  },
  "dependencies": {
    "@slack/client": "4.8.0",
    "debug": "4.1.0",
    "lodash": "4.17.11",
    "node-cache": "4.2.0",
    "request": "2.88.0",
    "request-promise-native": "1.0.5",
    "sonos": "1.5.1",
    "supports-color": "5.5.0",
    "snyk": "^1.161.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "snyk": true
}
